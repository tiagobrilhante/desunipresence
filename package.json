{
  "name": "nuxt-with-nuxtui",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "supabase:init": "pnpx supabase init",
    "supabase:login": "pnpx supabase login",
    "run:migrations": "pnpx supabase db push",
    "supabase:link": "pnpx supabase link --project-ref $SUPABASE_PROJECT_REF",
    "supabase:types": "pnpx supabase gen types typescript --project-id $SUPABASE_PROJECT_REF --schema public > ./app/types/database.types.ts",
    "supabase:types:linked": "pnpx supabase gen types typescript --linked --schema public > ./app/types/database.types.ts",
    "db:migrate:new": "pnpx supabase migration new",
    "db:reset": "pnpx supabase db reset --linked",
    "db:seed": "node --env-file=.env  supabase/seed/seed.js"
  },
  "dependencies": {
    "@nuxt/image": "1.11.0",
    "@nuxt/ui": "4.0.1",
    "@nuxtjs/supabase": "2.0.1",
    "@pinia/nuxt": "0.11.2",
    "@supabase/supabase-js": "^2.75.0",
    "@unovis/ts": "^1.6.1",
    "@unovis/vue": "^1.6.1",
    "@vueuse/nuxt": "^13.9.0",
    "date-fns": "^4.1.0",
    "nuxt": "^4.1.3",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.68",
    "@iconify-json/material-symbols": "^1.2.41",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/test-utils": "3.19.2",
    "eslint": "^9.37.0",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.0"
  },
  "packageManager": "pnpm@10.18.2+sha512.9fb969fa749b3ade6035e0f109f0b8a60b5d08a1a87fdf72e337da90dcc93336e2280ca4e44f2358a649b83c17959e9993e777c2080879f3801e6f0d999ad3dd"
}
